package model.answer;

import model.Interview;
import model.question.*;
 
public class TextAnswer extends Answer {

	private String text;

	public TextAnswer() {
		super();
 	}

 	public TextAnswer(Integer id, TextQuestion TextQuestion, Interview interview, String text) {
		super(id, TextQuestion, interview);
		this.text = text;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}
	

}
